  "6",
  1.0,
  {
    "equipment": "\u0442\u0430\u0431\u0443\u0440\u0435\u0442\u043a\u0430",
    "materials": "\u0414\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430, \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u043d\u0430\u044f \u043d\u0430 \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u0435 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u044c\u043d\u044b\u0445 \u0438 \u0434\u0443\u0445\u043e\u0432\u043d\u044b\u0445 \u0431\u043b\u0430\u0433, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0443\u0434\u043e\u0432\u043b\u0435\u0442\u0432\u043e\u0440\u044f\u044e\u0442 \u043f\u043e\u0442\u0440\u0435\u0431\u043d\u043e\u0441\u0442\u0438 \u0438\u043d\u0434\u0438\u0432\u0438\u0434\u0430 \u0438 \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0430.",
