  1.0,
  {
    "equipment": "\u0443\u0447\u0435\u0431\u043d\u0438\u043a",
    "materials": "\u042f\u0437\u044b\u043a \u0432\u043e\u0441\u0442\u043e\u0447\u043d\u043e\u0441\u043b\u0430\u0432\u044f\u043d\u0441\u043a\u043e\u0439 \u0433\u0440\u0443\u043f\u043f\u044b \u0441\u043b\u0430\u0432\u044f\u043d\u0441\u043a\u043e\u0439 \u0432\u0435\u0442\u0432\u0438 \u0438\u043d\u0434\u043e\u0435\u0432\u0440\u043e\u043f\u0435\u0439\u0441\u043a\u043e\u0439 \u044f\u0437\u044b\u043a\u043e\u0432\u043e\u0439 \u0441\u0435\u043c\u044c\u0438, \u043d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0439 \u044f\u0437\u044b\u043a \u0440\u0443\u0441\u0441\u043a\u043e\u0433\u043e \u043d\u0430\u0440\u043e\u0434\u0430.",
    "name": "\u0420\u0443\u0441\u0441\u043a\u0438\u0439 \u044f\u0437\u044b\u043a"
